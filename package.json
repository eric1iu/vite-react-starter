{
  "name": "vite-react-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write .",
    "lint": "tsc --noEmit && eslint ./ --ext .jsx,.js,.ts,.tsx --fix --ignore-path ./.gitignore",
    "start": "npm run dev -w starter-playground",
    "build": "npm run build -w starter-playground"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  },
  "license": "MIT",
  "author": "Eric Liu",
  "workspaces": [
    "./packages/*"
